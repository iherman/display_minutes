{
  "name": "@iherman/display-minutes",
  "description": "Generation of index and resolutions' files for minutes generated by W3C's minute generator script.",
  "version": "2.0.0",
  "exports": "./main.ts",
  "fmt": {
    "useTabs": true,
    "lineWidth": 120,
    "indentWidth": 4,
    "proseWrap": "preserve"
  },
  "tasks": {
    "publish" : "deno publish --no-check",
    "run" : "deno run --allow-sys --allow-read --allow-write --allow-env main.ts"
  },
